<?xml version='1.0' encoding='utf-8'?>
<nrml xmlns:gml="http://www.opengis.net/gml"
      xmlns="http://openquake.org/xmlns/nrml/0.3"
      gml:id="n1">

    <sourceModel id="sm1">

        <simpleFaultSource id="src01" name="Mount Diablo Thrust">
            <tectonicRegion>Active Shallow Crust</tectonicRegion>

            <simpleFaultGeometry>
                <gml:LineString srsName="urn:ogc:def:crs:EPSG::4326">
                    <gml:posList>
                        -121.82290 37.73010
                        -122.03880 37.87710
                    </gml:posList>
                </gml:LineString>
            </simpleFaultGeometry>

            <magnitudeScalingRelationship>WC1994</magnitudeScalingRelationship>
            <ruptureAspectRatio>1.5</ruptureAspectRatio>

            <upperSeismogenicDepth>10.0</upperSeismogenicDepth>
            <lowerSeismogenicDepth>10.0</lowerSeismogenicDepth>

            <!-- TODO(larsbutler): Do we need the 'type'? -->
            <evenlyDiscretizedIncrementalMFD minMag="5.0" binWidth="0.1" type="ML">
                <occurrenceRates>0.0010614989 8.8291627E-4 7.3437777E-4 6.108288E-4 5.080653E-4</occurrenceRates>
            </evenlyDiscretizedIncrementalMFD>

            <dip>45.0</dip>
            <rake>30.0</rake>

        </simpleFaultSource>

    </sourceModel>
</nrml>
