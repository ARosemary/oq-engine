<?xml version="1.0" encoding="utf-8"?>
<nrml
xmlns="http://openquake.org/xmlns/nrml/0.5"
xmlns:gml="http://www.opengis.net/gml"
>
    <uniformHazardSpectra
    investigationTime="50.0"
    poE="0.1"
    quantileValue="0.15"
    statistics="quantile"
    >
        <periods>
            0.0 0.025 0.1 0.2
        </periods>
        <uhs>
            <gml:Point>
                <gml:pos>
                    0.0 0.0
                </gml:pos>
            </gml:Point>
            <IMLs>
                0.3 0.5 0.2 0.1
            </IMLs>
        </uhs>
        <uhs>
            <gml:Point>
                <gml:pos>
                    0.0 1.0
                </gml:pos>
            </gml:Point>
            <IMLs>
                0.3 0.5 0.2 0.1
            </IMLs>
        </uhs>
    </uniformHazardSpectra>
</nrml>
